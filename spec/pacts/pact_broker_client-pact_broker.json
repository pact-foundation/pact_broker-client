        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/hal+json"
        },
        "body": {
          "_links": {
            "self": {
              "href": "http://example.org/pacts/latest"
            }
          },
          "pacts": [
            {
              "_links": {
                "self": [
                  {
                    "href": "http://example.org/pacts/provider/Pricing%20Service/consumer/Condor/latest"
