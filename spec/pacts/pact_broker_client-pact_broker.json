                  "_embedded": {
                    "version": {
                      "number": "1.3.0"
                    }
                  }
                },
                "provider": {
                  "_links": {
                    "self": {
                      "href": "http://example.org/pacticipants/Pricing%20Service"
                    }
                  },
                  "name": "Pricing Service"
                }
              }
            }
          ]
        },
        "matchingRules": {
          "$.headers.Content-Type": {
            "match": "regex",
